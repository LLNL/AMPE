// Input file
model_type = "Quat"

end_time            = 4000.
max_timesteps       = 100

run_name = "test"

Visit {
   interval = 25
   interval_type = "step"
   extra_output  = TRUE
}

TimerManager {
    timer_list = "AMPE::*::*"
    print_threshold = 0.0
}

ModelParameters {
   H_parameter = -1.

   three_phases = TRUE
   
   Temperature{
      temperature = 1000.
   }
   
   epsilon_phi = 1.

   phi_mobility = 1.

   phi_well_scale = 1. 

   ConcentrationModel {
      model    = "calphad"
      rhs_form = "ebs"
      Calphad {
         filename ="calphad3phases.json"
      }
      NewtonSolver {
        max_its = 50
        tol = 1.e-12
      }
      diffusion_type = "temperature_dependent"
      D_solid_A  =  3.e-1 //um**2/s
      D_solid_B  =  3.e-1 //um**2/s
      D_liquid  = 3.e3  //um**2/s
      Q0_solid_A  = 0.    // [J/mol]
      Q0_solid_B  = 0.    // [J/mol]
      Q0_liquid = 0.    // [J/mol]
   }

   molar_volume = 1.e-5 // [m^3/mol]

   BoundaryConditions {
       Phase{
         boundary_0 = "slope", "0"
         boundary_1 = "slope", "0"
         boundary_2 = "slope", "0"
         boundary_3 = "slope", "0"
         boundary_4 = "slope", "0"
         boundary_5 = "slope", "0"
      }
      Conc{
         boundary_0 = "slope", "0"
         boundary_1 = "slope", "0"
         boundary_2 = "slope", "0"
         boundary_3 = "slope", "0"
         boundary_4 = "slope", "0"
         boundary_5 = "slope", "0"
      }
   }
}

ScalarDiagnostics {
   interval = 10
   interval_type = "step"
}   

Integrator {
   atol = 1.e-5
   Preconditioner{
     enabled = FALSE
   }
}

InitialConditions {
   filename = "test.nc"
   init_cl = 0.5309
   init_ca = 0.7686
   init_cb = 0.2314
}

Geometry{
   periodic_dimension = 0, 0, 0
   coarsest_level_resolution = 16, 16, 16
   x_lo         =  0.,  0.,  0.
   x_up         =  50., 50., 50.
}
