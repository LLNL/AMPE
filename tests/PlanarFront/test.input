// Input file
model_type = "KWCcomplex"

end_time            = 50.
max_timesteps       = 5000

Visit {
   interval = 1.
   interval_type = "time"
}

TimerManager {
    timer_list = "solv::*::*","xfer::*::*","PFModel::*",
                 "QuatIntegrator::*","EllipticFACOps::*"
    print_threshold = 0.0
}

ModelParameters {
   H_parameter = -1.
   
   Temperature{
      meltingT = 921.12
      temperature = 920.87
      latent_heat = 9.5e3 // [J/mol*K]
   }
   
   epsilon_phi = 1.77669990185572 // sqrt(6.*delta*L*Gamma/Tm)

   phi_mobility = 1.985 // muk*Tm/(6.*delta*L)

   phi_well_scale =  0.022017765260778047 // ((eps/delta)**2)/32
   phi_well_func_type = "double"

   phi_interp_func_type ="pbg"

   FreeEnergyModel{   
      type = "linear"
   }

   molar_volume = 1.e-5 // [m^3/mol]

   BoundaryConditions {
       Phase{
         boundary_0 = "slope", "0"
         boundary_1 = "slope", "0"
         boundary_2 = "slope", "0"
         boundary_3 = "value", "0"
      }
   }
}

ScalarDiagnostics {
   interval = 1.
   interval_type = "time"
}   

Integrator {
   atol = 1.e-5
   Preconditioner{
//     enabled = FALSE
   }
}

InitialConditions {
   filename = "test.nc"
}

Geometry{
   periodic_dimension = 0, 0
   coarsest_level_resolution = 64, 512
   x_lo         =  0.,    0. 
   x_up         =  50., 400.
}
