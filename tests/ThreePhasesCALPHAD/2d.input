// Input file
end_time            = 140.
max_timesteps       = 750

run_name = "test"

Visit {
   interval = 100
   interval_type = "step"
}

TimerManager {
    timer_list = "AMPE::*::*"
    print_threshold = 0.0
}

ModelParameters {

   three_phases = TRUE
   
   Temperature{
      type = "scalar"
      temperature = 1.e-8
   }
   
   epsilon_phi = 1.77669990185572 // sqrt(6.*delta*L*Gamma/Tm)
   PhaseMobility{
      value = 1.985 // muk*Tm/(6.*delta*L)
   }
   phi_well_scale =  0.022017765260778047 // ((eps/delta)**2)/32

   ConcentrationModel {
      model    = "calphad"
      rhs_form = "ebs"
      Calphad {
         filename ="calphad3phases.json"
      }
      NewtonSolver {
        max_its = 50
        tol = 1.e-12
      }
      diffusion_type = "temperature_dependent"
      D_solid_A  =  3.e-1 //um**2/s
      D_solid_B  =  3.e-1 //um**2/s
      D_liquid  = 3.e3  //um**2/s
      Q0_solid_A  = 0.    // [J/mol]
      Q0_solid_B  = 0.    // [J/mol]
      Q0_liquid = 0.    // [J/mol]
   }

   molar_volume = 1.e-5 // [m^3/mol]

   BoundaryConditions {
       Phase{
         boundary_0 = "slope", "0"
         boundary_1 = "slope", "0"
         boundary_2 = "slope", "0"
         boundary_3 = "slope", "0"
      }
      Conc{
         boundary_0 = "slope", "0"
         boundary_1 = "slope", "0"
         boundary_2 = "slope", "0"
         boundary_3 = "slope", "0"
      }
   }
}

ScalarDiagnostics {
   interval = 20.
   interval_type = "time"
}   

Integrator {
   atol = 1.e-5
   Preconditioner{
     enabled = FALSE
   }
}

InitialConditions {
   filename = "test.nc"
   init_cl = 0.5309
   init_ca = 0.7686
   init_cb = 0.2314
}

Geometry{
   periodic_dimension = 0, 0
   coarsest_level_resolution = 32, 32
   x_lo         =  0.,    0. 
   x_up         =  100., 100.
}
